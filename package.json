{
  "name": "@radixnameservice/rns-sdk",
  "version": "3.3.0",
  "description": "The official toolkit for the Radix Name Service",
  "type": "module",
  "scripts": {
    "test": "jest",
    "dev": "nodemon",
    "start": "node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node ./src/index.ts",
    "build": "tsup"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "@radixdlt/babylon-gateway-api-sdk": "^1.9.2",
    "bech32": "^2.0.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "decimal.js": "^10.5.0"
  },
  "devDependencies": {
    "@radixdlt/radix-dapp-toolkit": "^2.2.1",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
