{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "es2019",
        "lib": ["es2019"],
        "esModuleInterop": true,
        "declaration": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "outDir": "dist",
        "skipLibCheck": true,
        "sourceMap": true
    },
    "include": [
        "src/**/*"
    ],
}